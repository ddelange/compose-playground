{
  "HTTP": {
    "Namespace": "works.owncloud"
  },
  "policy_selector": {
    "static": {"policy" : "reva"}
  },
  "policies": [
    {
      "name": "reva",
      "routes": [
        {
          "endpoint": "/",
          "backend": "https://ocis-bridge.owncloud.works"
        },
        {
          "endpoint": "/.well-known/",
          "backend": "https://ocis-bridge.owncloud.works"
        },
        {
          "endpoint": "/konnect/",
          "backend": "https://ocis-bridge.owncloud.works"
        },
        {
          "endpoint": "/signin/",
          "backend": "https://ocis-bridge.owncloud.works"
        },
        {
          "endpoint": "/ocs/",
          "backend": "https://ocis-bridge.owncloud.works"
        },
        {
          "endpoint": "/remote.php/",
          "backend": "https://ocis-bridge.owncloud.works"
        },
        {
          "endpoint": "/dav/",
          "backend": "https://ocis-bridge.owncloud.works"
        },
        {
          "endpoint": "/webdav/",
          "backend": "https://ocis-bridge.owncloud.works"
        },
        {
          "endpoint": "/status.php",
          "backend": "https://ocis-bridge.owncloud.works"
        },
        {
          "endpoint": "/index.php/",
          "backend": "https://ocis-bridge.owncloud.works"
        }
      ]
    },
    {
      "name": "oc10",
      "routes": [
        {
          "endpoint": "/",
          "backend": "https://ocis-bridge.owncloud.works"
        },
        {
          "endpoint": "/.well-known/",
          "backend": "https://ocis-bridge.owncloud.works"
        },
        {
          "endpoint": "/konnect/",
          "backend": "https://ocis-bridge.owncloud.works"
        },
        {
          "endpoint": "/signin/",
          "backend": "https://ocis-bridge.owncloud.works"
        },
        {
          "endpoint": "/ocs/",
          "backend": "https://oc10-bridge.owncloud.works",
          "apache-vhost": true
        },
        {
          "endpoint": "/remote.php/",
          "backend": "https://oc10-bridge.owncloud.works",
          "apache-vhost": true
        },
        {
          "endpoint": "/dav/",
          "backend": "https://oc10-bridge.owncloud.works",
          "apache-vhost": true
        },
        {
          "endpoint": "/webdav/",
          "backend": "https://oc10-bridge.owncloud.works",
          "apache-vhost": true
        },
        {
          "endpoint": "/status.php",
          "backend": "https://oc10-bridge.owncloud.works"
        },
        {
          "endpoint": "/index.php/",
          "backend": "https://oc10-bridge.owncloud.works"
        }
      ]
    }
  ]
}